<h1>Notes#index</h1>

<h1>All Notes</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Note</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <%@notes.each do |note| %>
      <tr>
        <td><%= note.title %></td>
        <td><%= note.content%></td>
       </tr>
       <tr>
        <td><%= link_to 'Show', note %></td>
        <td><%= link_to 'Edit', edit_note_path(note) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= link_to 'Add note', new_note_path %>

<p>Find me in app/views/notes/index.html.erb</p>
